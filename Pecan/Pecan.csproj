<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>0.0.1</Version>
    <Authors>Ben Owen</Authors>
    <Copyright>Copyright 2021 Ben Owen</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/LordBenjamin/pecan</PackageProjectUrl>
    <RepositoryUrl>https://github.com/LordBenjamin/pecan</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Pecan, web, server, http, meadow</PackageTags>
    <Description>Pecan is a minimal and lightweight web API server for Meadow and similar embedded .NET environments.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Buffers" Version="4.5.1" />
  </ItemGroup>

</Project>
